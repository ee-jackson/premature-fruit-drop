@article{reichardtPeptideSignalingDroughtinduced2020,
  title = {Peptide Signaling for Drought-Induced Tomato Flower Drop},
  author = {Reichardt, S. and Piepho, H.-P. and Stintzi, A. and Schaller, A.},
  year = {2020},
  month = mar,

@article{tiansawatPredispersalSeedPredators2017,
  title = {Pre-Dispersal Seed Predators and Fungi Differ in Their Effect on {{Luehea}} Seemannii Capsule Development, Seed Germination, and Dormancy across Two Panamanian Forests},

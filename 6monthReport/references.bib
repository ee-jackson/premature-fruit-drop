@article{snowCostsFlowerFruit1989,
  title = {Costs of Flower and Fruit Production in Tipularia Discolor ({{Orchidaceae}})},
  author = {Snow, Allison A. and Whigham, Dennis F.},
  year = {1989},
  volume = {70},
  pages = {1286--1293},
  issn = {1939-9170},
  doi = {10.2307/1938188},
  abstract = {The cost of reproduction may be an important constraint on the evolution of life history traits, yet it has seldom been adequately measured in plants. To demonstrate a net cost of reproductive structures one must show that their production has a negative effect on future growth. Using a summer\textemdash{}deciduous orchid, we compared subsequent growth of plants with no inflorescence, no fruits, few fruits ({$<$}10), or many fruits ({$>$}10). Greater investment in reproduction led to increasingly negative effects on future corm size and leaf area. The cost of an inflorescence without fruits was about one\textemdash{}half the cost of an inflorescence with few fruits. Each fruit cost the plant \textdegree{}2\% of its total leaf area the next year. Natural pollination resulted in a mean of seven fruits per plant. Hand\textemdash{}pollination led to a dramatic increase in fruit set (up to 25 fruits per plant), and a large decrease in subsequent growth. Plants with many fruits were less likely to propagate vegetatively than those with few fruits. Also, plants with many fruits were less likely to flower the following year, probably because they were smaller than those with few fruits. The potential benefits of greater pollinator visitation are unclear because larger 'clutch size' may not result in greater lifetime fecundity.},
  copyright = {\textcopyright{} 1989 by the Ecological Society of America},
  file = {/Users/eleanorjackson/OneDrive - University of Reading/Zotero/snow_whigham_1989_costs of flower and fruit production in tipularia discolor (orchidaceae).pdf},
  journal = {Ecology},
  language = {en},
  note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.2307/1938188},
  number = {5}
}

@article{steffan-dewenterPollinationSeedSet2001,
  title = {Pollination, Seed Set and Seed Predation on a Landscape Scale},
  author = {{Steffan-Dewenter}, Ingolf and M{\"u}nzenberg, Ute and Tscharntke, Teja},
  year = {2001},
  month = aug,
  volume = {268},
  pages = {1685--1690},
  publisher = {{Royal Society}},
  doi = {10.1098/rspb.2001.1737},
  abstract = {We analysed the combined effects of pollination and seed predation on seed set of Centaurea jacea in 15 landscapes differing in structural complexity. In the centre of each landscape, a patch of Centaurea plants was established for standardized measurements of flower visitation, seed predation and seed set. Both the number of flower\textendash{}visiting bees and the proportion of flower heads damaged by seed predators increased with landscape complexity, which was measured as the proportion of semi\textendash{}natural habitats. The mean number of seeds per flower head did not increase with the proportion of semi\textendash{}natural habitats, presumably because of the counterbalancing effects of pollination and seed predation. For a subset of undamaged flower heads, the number of seeds per flower head was positively correlated with the number of flower visits. Further reasons for the unexpected failure to detect a correlation between landscape complexity and seed set appeared to be changes in flower\textendash{}visitor behaviour and the contrasting responses of honeybees and wild bees to habitat context. Landscape analyses at eight spatial scales (radius of landscape sectors, 250\textendash{}3000 m) showed that different groups perceived the landscape at different spatial scales. Changes in pollinator numbers could be explained only at small scales (up to 1000 m), while the seed predators also responded to large scales (up to 2500 m).},
  file = {/Users/eleanorjackson/OneDrive - University of Reading/Zotero/steffan-dewenter et al_2001_pollination, seed set and seed predation on a landscape scale.pdf},
  journal = {Proceedings of the Royal Society of London. Series B: Biological Sciences},
  number = {1477}
}

